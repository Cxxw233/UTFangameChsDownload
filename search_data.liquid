layout: null
permalink: search.json
---

[
  "translations": [
  {%- for post in site.translations -%}
  {
    "title": {{ post.title | jsonify }},
    "authors": [
      {%- for author in post.authors -%}
      {%- assign author_post = site.authors | find: "slug", author -%}
      {{ author_post.title | jsonify }}{%- unless forloop.last -%},{%- endunless -%}
      {%- endfor -%}
    ],
    "content": {{ post.output | jsonify }}
  }{%- unless forloop.last -%},{%- endunless -%}
  {%- endfor -%}
  ],
  "authors": [
    {%- for author in site.authors -%}
    {{ author.title | jsonify }}{%- unless forloop.last -%},{%- endunless -%}
    {%- endfor -%}
  ]
]
