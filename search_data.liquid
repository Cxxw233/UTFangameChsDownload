---
layout: null
permalink: search.json
---
{ "translations": [
{%- for post in site.translations -%}
	{ "title": {{ post.title | jsonify }}, "authors": {{ post.authors | jsonify }}, "links":
	{{ post.links | jsonify }} }
	{%- unless forloop.last -%},{%- endunless -%}
{%- endfor -%}
], "authors": {
{%- for author in site.authors -%}
	{{ author.slug | jsonify }}: {{ author.title | jsonify }}
	{%- unless forloop.last -%},{%- endunless -%}
{%- endfor -%}
} }
